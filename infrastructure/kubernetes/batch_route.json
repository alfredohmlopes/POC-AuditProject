{
    "uri": "/v1/events/batch",
    "methods": [
        "POST"
    ],
    "upstream": {
        "type": "roundrobin",
        "nodes": {
            "vector.vector.svc.cluster.local:8080": 1
        }
    },
    "plugins": {
        "key-auth": {
            "header": "X-API-Key"
        },
        "limit-count": {
